<%= form_for @user, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="form-group">
    <%= f.label :name, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :name, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :role, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :role, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :job, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :job, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :phone, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :phone, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :email, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :email, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :deleted_at, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :deleted_at, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :encrypted_password, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :encrypted_password, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :reset_password_token, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :reset_password_token, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :reset_password_sent_at, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :reset_password_sent_at, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :remember_created_at, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :remember_created_at, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :current_sign_in_at, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :current_sign_in_at, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :last_sign_in_at, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :last_sign_in_at, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :current_sign_in_ip, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :current_sign_in_ip, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :last_sign_in_ip, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :last_sign_in_ip, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :failed_attempts, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :failed_attempts, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :unlock_token, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :unlock_token, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :locked_at, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :locked_at, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :role_id, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :role_id, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :admin, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :admin, :class => 'text_field form-control' %>
    </div>
  </div>

  <div class="form-group">
    <div class='col-md-offset-2 col-md-10'>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  users_path, :class => 'btn btn-default' %>
    </div>
  </div>
<% end %>
